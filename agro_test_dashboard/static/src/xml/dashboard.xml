<!--?xml version="1.0" encoding="UTF-8"?-->                            
<templates xml:space="preserve">
   <t t-name="DashboardDashboard">
      <div class="oh_dashboards">
          <div class="container-fluid my-5 o_hr_dashboard"/>
      </div>
   </t>
   <t t-name="MainSection">
      <section class="dashboard_main" id="main_section">
          <div class="col-sm-12 mb-4">
               <div class="row">
                  <div class="col-12 col-sm-12 col-md-8">
                      <h2 class="section-header">Dashboard Sales</h2>
                  </div>
                  <div class="col-sm-12 mb-3">
                      <div class="row">
                          <div class="col-md-3 col-sm-5 storable_products oh-data" t-on-click="goBackend">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#aaff80">
                                              <i class="fa fa-files-o fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Quotations</div>
                                              <div class="stat_count">
                                                  <span id="storable">
                                                      <div id="total_quotation"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 all_products oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#ff4d94">
                                              <i class="fa fa-line-chart fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Sales Order</div>
                                              <div class="stat_count">
                                                  <span id="templates">
                                                      <div id="total_order"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 product_variants oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#99e6ff">
                                              <i class="fa fa-money fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Total Purchase Amount</div>
                                              <div class="stat_count">
                                                  <span id="count">
                                                      <div id="total_order_amount"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 consu_products oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#bf80ff">
                                              <i class="fa fa-users fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Customers</div>
                                              <div class="stat_count">
                                                  <span id="consumable">
                                                      <div id="total_customer"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
              <div class="row">
                  <div class="col-12 col-sm-12 col-md-8">
                      <h2 class="section-header">Dashboard Purchase</h2>
                  </div>
                  <div class="col-sm-12 mb-3">
                      <div class="row">
                          <div class="col-md-3 col-sm-5 storable_products oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#aaff80">
                                              <i class="fa fa-star fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Priority</div>
                                              <div class="stat_count">
                                                  <span id="storable">
                                                      <div id="total_priority"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 all_products oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#ff4d94">
                                              <i class="fa fa-shopping-cart fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Purchase Order</div>
                                              <div class="stat_count">
                                                  <span id="templates">
                                                      <div id="total_purchase"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 product_variants oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#99e6ff">
                                              <i class="fa fa-money fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Total Order Amount</div>
                                              <div class="stat_count">
                                                  <span id="count">
                                                      <div id="total_purchase_amount"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3 col-sm-5 consu_products oh-data">
                              <div class="oh-card" style="width: 470px;">
                                  <div class="oh-card-body">
                                      <div class="stat-widget-one">
                                          <div class="stat-icon" style="background:#bf80ff">
                                              <i class="fa fa-users fa-2xl"/>
                                          </div>
                                          <div class="stat-content">
                                              <div class="stat-head">Vendors</div>
                                              <div class="stat_count">
                                                  <span id="consumable">
                                                      <div id="total_supplier"/>
                                                  </span>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </section>
   </t>
</templates>